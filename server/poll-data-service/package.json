{
  "name": "poll-data-service",
  "version": "1.0.0",
  "description": "Application that grabs data from an open API at scheduled intervals and inserts to a DB",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "nodemon",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles --coverage=true",
    "build": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.9"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1"
  }
}
